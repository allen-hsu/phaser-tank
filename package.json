{
  "name": "phaser3-es6-webpack4",
  "version": "1.0.0",
  "description": "Plain webpack 4 Boilerplate with Babel, SASS and Phaser3 on board",
  "author": "Rafael Delboni",
  "license": "MIT",
  "homepage": "https://github.com/rafaeldelboni/phaser3-es6-webpack4.git",
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack/config.dev.js",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --progress --mode development --config webpack/config.dev.js",
    "build": "webpack --progress --mode production --config webpack/config.build.js",
    "lint": "eslint ./src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rafaeldelboni/phaser3-es6-webpack4.git"
  },
  "dependencies": {
    "phaser": "^3.3.0",
    "phaser-sprite-gui": "^0.7.1",
    "rxjs": "^5.5.10"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta",
    "browser-sync": "^2.23.6",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "expose-loader": "^0.7.5",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.8.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-merge": "^4.1.2"
  }
}
